<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta http-equiv="X-UA-Compatible" />
    <title>Chat app</title>
    <link href="/style.css" rel="stylesheet" type="text/css">
    <script src="/socket.io/socket.io.js"></script>
    <script src="https://unpkg.com/peerjs@1.3.1/dist/peerjs.min.js"></script>
    <link rel="icon" type="image/png" href="http://example.com/favicon.png">
  </head>
  <body class="body">
    <header>
      <img class="logo" src="https://i.imgur.com/qXe7lWg.png">
    </header>
    <main id="app">
      <div class="form" >
        <form class="form" action="/room" method="POST">
          <input class="input" type="text" name="room" required autocomplete="off"/>
          <button class="create" type="submit">New Room</button>
        </form>
      </div>
      <ul id="room-container">
          <% Object.keys(rooms).forEach(room => { %>
            <li>
            <form  action="/<%= room %>">
              <input class="roomNameList" type="submit" value= "<%= room %>" />
             </form>
            <% }) %>
        </li>
      </ul>
    </main>
    <script src="script.js" defer></script>
  </body>
</html>
